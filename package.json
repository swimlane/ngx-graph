    "@angular/cli": "^10.1.2",
    "@angular/compiler-cli": "^10.1.2",
    "@angular/language-service": "^10.1.2",
    "@babel/core": "^7.7.7",
    "@swimlane/prettier-config-swimlane": "^3.0.2",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/mdast": "^3.0.3",
    "@types/node": "^12.12.34",
    "@types/unist": "^2.0.2",
    "@types/vfile": "^4.0.0",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.2.2",
    "cross-env": "^7.0.2",
    "husky": "^4.2.4",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
