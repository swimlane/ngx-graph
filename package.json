    "ts-node": "^8.8.2",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-security": "^1.16.0",
    "tslint-config-swimlane": "^5.0.0",
    "typescript": "~3.9.7"
  },
  "browser": {
